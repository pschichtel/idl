            "BotUrl": {
                "type": [
                    "string"
                ],
                "description": "URL to the server that implements the Bot API.",
                "example": "http://echo-bot:9090",
                "examples": [
                    "http://echo-bot:9090"
                ],
                "format": "uri"
            },
            "BotToken": {
                "type": [
                    "string"
                ],
                "description": "A bearer token that is used for calls to the Bot API.",
                "examples": [
                ]
